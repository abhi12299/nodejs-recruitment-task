    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.8.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "sqlite3": "^5.0.8",
    "swagger-ui-express": "^4.3.0",
